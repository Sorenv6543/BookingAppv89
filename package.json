{
  "name": "property-cleaning-scheduler",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/remove-dev-manifest.js && vite",
    "build": "vue-tsc --noEmit && vite build && node scripts/format-manifest.js && NODE_ENV=production node scripts/remove-dev-manifest.js",
    "build:fast": "vite build",
    "build:analyze": "vue-tsc --noEmit && vite build --mode analyze",
    "build:production": "set NODE_ENV=production && vite build",
    "build:owner-only": "set ROLE_BUILD=owner && vite build",
    "build:admin-only": "set ROLE_BUILD=admin && vite build",
    "build:pwa": "vite build && node scripts/format-manifest.js && node scripts/optimize-pwa.js",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "preview:pwa": "vite preview --host",
    "lint": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:pwa": "vitest run --reporter=verbose src/__tests__/**/*pwa*",
    "test:performance": "vitest run src/__tests__/utils/performance.spec.ts && vitest run src/__tests__/utils/performance-regression.spec.ts",
    "test:performance:watch": "vitest src/__tests__/utils/performance.spec.ts src/__tests__/utils/performance-regression.spec.ts",
    "analyze:bundle": "pnpx vite-bundle-analyzer dist",
    "analyze:pwa": "node scripts/optimize-pwa.js",
    "perf:monitor": "node scripts/optimize-pwa.js full",
    "perf:bundle": "node scripts/performance-bundle-analyzer.js",
    "perf:imports": "node scripts/optimize-pwa.js imports",
    "perf:regression": "vitest run src/__tests__/utils/performance-regression.spec.ts",
    "perf:analysis": "npm run perf:bundle && npm run perf:regression",
    "perf:report": "npm run perf:analysis && echo 'Performance analysis complete. See performance-bundle-report.json for details.'",
    "mcp:browser-tools": "node scripts/start-browser-tools-mcp.js",
    "mcp:browser-tools:basic": "npx browser-tools-mcp",
    "mcp:browser-tools:enhanced": "npx @eqiz/browser-tools-mcp-enhanced",
    "mcp:browser-tools:test": "node scripts/test-browser-tools.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/list": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@fullcalendar/vue3": "^6.1.20",
    "@mdi/font": "^7.4.47",
    "@mdit/plugin-uml": "^0.23.0",
    "@supabase/supabase-js": "^2.93.0",
    "dotenv": "^17.2.3",
    "lint-staged": "^16.2.7",
    "markdown-it-plantuml": "^1.4.1",
    "ngrx-uml": "^1.0.2",
    "pinia": "^3.0.4",
    "prettier": "^3.8.1",
    "supabase": "^2.72.8",
    "uml": "^1.0.0",
    "uuid": "^13.0.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "vuetify": "3.11.7",
    "workbox-build": "^7.4.0"
  },
  "devDependencies": {
    "@eqiz/browser-tools-mcp-enhanced": "^3.0.1",
    "@eslint/js": "^9.39.2",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "4.0.18",
    "@vue/test-utils": "^2.4.6",
    "browser-tools-mcp": "^1.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.1.0",
    "happy-dom": "^20.3.9",
    "jsdom": "^27.4.0",
    "sass-embedded": "^1.97.3",
    "typescript": "~5.9.3",
    "v4": "^0.0.1",
    "vite": "^7.3.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-vuetify": "^2.1.3",
    "vitest": "^4.0.18",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.4",
    "vuetify": "3.11.7",
    "workbox-window": "^7.4.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
