{
	"servers": {
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"."
			],
			"type": "stdio"
		},

		"github": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@modelcontextprotocol/server-github"
			],
			"env": {
				"GITHUB_PERSONAL_ACCESS_TOKEN": "${env:local:GITHUB_PERSONAL_ACCESS_TOKEN}"
			},
			"type": "stdio"
		},
		"fetch": {
			"command": "npx",
			"args": [
				"-y",
				"fetch-mcp"
			],
			"type": "stdio"
		},
		"brave-search": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-brave-search"
			],
			"env": {
				"BRAVE_API_KEY": "${env:local:BRAVE_API_KEY}"
			},
			"type": "stdio"
		},
		"desktop-commander": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@wonderwhy-er/desktop-commander"
			],
			"type": "stdio"
		},
		"supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "sbp_4ed7baf5bec159dd24b17e83ef705906d1ffcecd"
      ]
    }
	}
}